<launch>
    <!-- Run the ekf for map to odom config -->
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_with_gps" clear_params="true">
        <rosparam command="load" file="$(find aidboat_navigation)/params/aidboat_localization_config.yaml" />

    <!-- Placeholder for output topic remapping you either remap on this node that takes in the data, or both 
        <remap from="odometry/filtered" to=""/>
        <remap from="accel/filtered" to=""/>-->
    </node>
</launch>